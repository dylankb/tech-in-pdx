<%= render "layouts/errors", :object => @user %>

<%= form_for log_in_path, remote: true do |f| %>
  <div class="form-group">
    <%= f.label :email %>
    <%= f.text_field :email, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :password %>
    <%= f.password_field :password, class: "form-control" %>
  </div>
  <%= f.submit "Log in" %>
<% end %>
