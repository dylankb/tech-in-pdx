<%= render "layouts/errors", :object => @company %>

<%= form_for @company do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :technologies %>
  <%= f.text_field :technologies %>
  <%= f.label :street_1 %>
  <%= f.text_field :street_1 %>
  <%= f.label :street_2 %>
  <%= f.text_field :street_2 %>
  <%= f.label :city %>
  <%= f.text_field :city %>
  <%= f.label :state %>
  <%= f.text_field :state %>
  <%= f.label :zip %>
  <%= f.text_field :zip %>
  <%= f.submit %>
<% end %>
